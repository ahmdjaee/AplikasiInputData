<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rowLayout2"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_id2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:paddingVertical="4dp"
        android:textAlignment="center"
        android:textColor="#E3E2E2"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_warga2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.1"
        tools:text="ID" />

    <TextView
        android:id="@+id/tv_tanggal2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:paddingVertical="4dp"
        android:textColor="#E3E2E2"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_uang2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_warga2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.3"
        tools:text="Tanggal Iuran" />

    <TextView
        android:id="@+id/tv_uang2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingVertical="4dp"
        android:textColor="#E3E2E2"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_tanggal2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.3"
        tools:text="Uang Iuran" />

    <TextView
        android:id="@+id/tv_warga2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingVertical="4dp"
        android:textColor="#E3E2E2"
        android:textSize="16dp"
        app:layout_constraintEnd_toStartOf="@+id/tv_tanggal2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_id2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.3"
        tools:text="Nama Warga" />

    <TextView
        android:id="@+id/tvTextKeterangan"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingVertical="4dp"
        android:text="Keterangan : "
        android:textColor="#E3E2E2"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_tanggal2"
        app:layout_constraintStart_toEndOf="@+id/tv_id2"
        app:layout_constraintTop_toBottomOf="@+id/tv_warga2" />

    <TextView
        android:id="@+id/tv_ket"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingVertical="4dp"
        android:textColor="#E3E2E2"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_tanggal2"
        app:layout_constraintTop_toTopOf="@+id/tvTextKeterangan" />

</androidx.constraintlayout.widget.ConstraintLayout>